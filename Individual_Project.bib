@article{Galassi2012,
abstract = {High injection pressure is combined with high refueling rate for vehicles storing pressurized gaseous hydrogen onboard. As a drawback, high temperatures are developed inside the tank, which can jeopardize the structural integrity of the storage system. Computational Fluid Dynamics (CFD) codes already proved to be a valuable tool for predicting the temperature distribution within the tank during fast refueling. Results of hydrogen fast filling CFD simulations for a type IV tank, filled to 70 MPa at different working conditions are presented as follow up of the CFD model validation performed against experimental data. Alternative rates of pressure rise, adiabatic and cold filling are investigated to evaluate the effect on maximum hydrogen temperatures inside the tank. Results confirmed that the developed CFD model could be a suitable tool for investigating fast filling scenarios when experimental data are not yet available or of difficult realization.},
author = {Galassi, M. Cristina and Baraldi, Daniele and {Acosta Iborra}, Beatriz and Moretto, Pietro},
doi = {10.1016/J.IJHYDENE.2012.01.041},
file = {:Users/Pau/Library/Application Support/Mendeley Desktop/Downloaded/Galassi et al. - 2012 - CFD analysis of fast filling scenarios for 70 MPa hydrogen type IV tanks.pdf:pdf},
issn = {0360-3199},
journal = {International Journal of Hydrogen Energy},
month = {apr},
number = {8},
pages = {6886--6892},
publisher = {Pergamon},
title = {{CFD analysis of fast filling scenarios for 70 MPa hydrogen type IV tanks}},
url = {https://www.sciencedirect.com/science/article/pii/S0360319912000912{\#}fig4},
