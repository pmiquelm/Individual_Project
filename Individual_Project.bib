file = {:Users/Pau/Library/Application Support/Mendeley Desktop/Downloaded/Na Ranong et al. - 2011 - Approach for the Determination of Heat Transfer Coefficients for Filling Processes of Pressure Vessels With Co.pdf:pdf},
issn = {1521-0537},
journal = {Heat Transfer Engineering},
number = {2},
